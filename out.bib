  note = {arXiv:1307.1992v1},
  abstract = {We derive a Lieb-Robinson bound for the propagation of spin correlations
	in a model of spins interacting through a bosonic lattice field,
	which satisfies itself a Lieb-Robinson bound in the absence of spin-boson
