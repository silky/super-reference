}
@ARTICLE{Qian2013,
  author = {X. -F. Qian and J. H. Eberly},
  title = {Entanglement is Sometimes Enough},
  journal = {arXiv:1307.3772v1},
  year = {2013},
  month = {7},
  note = {arXiv:1307.3772v1},
  abstract = {For many decades the word "entanglement" has been firmly attached
