  month = {3},
  note = {arXiv:1403.2635v2},
  abstract = {Integrated quantum photonics is a promising approach for future practical
	and large-scale quantum information processing technologies, with
	the prospect of on-chip generation, manipulation and measurement
	of complex quantum states of light. The gallium arsenide (GaAs) material
	system is a promising technology platform, and has already successfully
	demonstrated key components including waveguide integrated single-photon
	sources and integrated single-photon detectors. However, quantum
	circuits capable of manipulating quantum states of light have so
	far not been investigated in this material system. Here, we report
	GaAs photonic circuits for the manipulation of single-photon and
	two-photon states. Two-photon quantum interference with a visibility
	of 94.9 +/- 1.3% was observed in GaAs directional couplers. Classical
	and quantum interference fringes with visibilities of 98.6 +/- 1.3%
	and 84.4 +/- 1.5% respectively were demonstrated in Mach-Zehnder
	interferometers exploiting the electro-optic Pockels effect. This
	work paves the way for a fully integrated quantum technology platform
	based on the GaAs material system.},
  file = {Wang2014, 1403.2635.pdf:Wang2014, 1403.2635.pdf:PDF},
  timestamp = {2014.04.21},
  url = {http://arxiv.org/abs/1403.2635v2}
}
@ARTICLE{Wang2014,
