  title = {Photon Number Splitting of Squeezed Light by a Single Qubit in Circuit
	QED},
  journal = {arXiv:1310.4034v1},
  year = {2013},
  month = {10},
  note = {arXiv:1310.4034v1},
  abstract = {We theoretically propose an efficient way to generate and detect squeezed
