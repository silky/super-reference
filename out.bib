@ARTICLE{Wang2013i,
  author = {Heming Wang and Hengyun Zhou and Liangzhu Mu and Heng Fan},
  title = {No-signaling Bell Equality and "Guess Your Neighbor's Input"},
  journal = {arXiv:1312.4648v1},
  year = {2013},
  month = {12},
  note = {arXiv:1312.4648v1},
  abstract = {Quantum mechanics is nonlocal. But it is not more nonlocal than no-signaling
	since of the principle of causality. Conversely, the no-signaling
	correlations are more nonlocal than those allowed in quantum mechanics.
	This is demonstrated in an enlightening multipartite game, "guess
	your neighbor's input" (GYNI). Yet, it is challenging to fully solve
	this game, only the simplest tripartite case is solved analytically
	under a specified assumption. Here, we propose the conceptually new
	Bell equality which is satisfied by all no-signaling correlations
	including quantum and classical correlations as special cases. By
	applying those Bell equalities, we provide a general framework in
	solving the GYNI scheme and confirm its validity for the number of
	players up to 19. Particularly, the tripartite case is fully solved
	analytically which provides a computable measure of supra-quantum
	correlation. The proposed Bell equality provides a precise principle
	for all no-signaling correlations.},
  file = {Wang2013, 1312.4648.pdf:Wang2013, 1312.4648.pdf:PDF},
  timestamp = {2014.01.05},
  url = {http://arxiv.org/abs/1312.4648v1}
}
@ARTICLE{Wang2014b,
