@ARTICLE{Wang2013h,
  author = {Guoming Wang},
  title = {Quantum algorithms for approximating the effective resistances of
	electrical networks},
  journal = {arXiv:1311.1851v2},
  year = {2013},
  month = {11},
  note = {arXiv:1311.1851v2},
  abstract = {The theory of electrical networks has many applications in algorithm
	design and analysis. It is an important problem to compute the basic
	quantities about electrical networks, such as electrical flows and
	effective resistances, as fast as possible. Classically, to compute
	these quantities, one need to solve a Laplacian system, and the best
	known algorithm takes nearly-linear time in the number of edges in
	the graph. In this paper, we present two quantum algorithms for approximating
	the effective resistance between any two vertices in a weighted graph.
	Both of them have time complexity $\poly{\log{n},d,c,1/\phi,1/\epsilon}$,
	where $n$ is the number of vertices in the graph, $d$ is the maximum
	degree of the graph, $c$ is the ratio of the maximum edge weight
	to the minimum edge weight, $\phi$ is the expansion of the graph,
	and $\epsilon$ is the relative error. The dependence of our algorithms
	on the graph's size is exponentially better than that of known classical
	algorithms. Furthermore, as by-products, our algorithms also yield
	good approximations of electrical flows and electrical potentials
	(as quantum states) respectively, which might be of independent interest.
	Finally, we give evidence that the polynomial dependence of our algorithms
	on the inverse expansion is necessary. Our algorithms are based on
	the technique of using quantum tools to study the algebraic properties
	of various matrices in spectral graph theory. While the first algorithm
	relies on implementing the projection onto the kernel of a weighted
	incidence matrix, and the second one relies on inverting the Laplacian
	matrix. It is hopeful that more quantum algorithms could be devised
	in similar way.},
  file = {Wang2013h, 1311.1851.pdf:Wang2013h, 1311.1851.pdf:PDF},
  timestamp = {2013.11.22},
  url = {http://arxiv.org/abs/1311.1851v2}
}
@ARTICLE{Wang2013b,
  author = {Hefeng Wang},
  title = {A quantum algorithm for obtaining the energy spectrum of a physical
	system using a resonating probe qubit},
  journal = {arXiv:1306.6732v1},
  year = {2013},
  month = {6},
  note = {arXiv:1306.6732v1},
  abstract = {We present a quantum algorithm for obtaining the energy spectrum of
	a physical system. A qubit is coupled to a $n$-qubit quantum register
	that represents the system to form a register $R$. We prepare $R$
	in a general reference state and construct a general operator that
	excites $R$ from the reference state to the state space of the system.
	A probe qubit is coupled to $R$ such that the probe exhibits a dynamical
	response only when it resonates with a transition between the reference
	state and the energy eigenstates of the system. By varying the probe's
	frequency, the energy spectrum and the eigenstates of the system
	can be obtained. In this algorithm, no initial guess on the eigenstate
	of the system is required.},
  file = {Wang2013b, 1306.6732.pdf:Wang2013b, 1306.6732.pdf:PDF},
