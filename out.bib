@ARTICLE{Brandao2013a,
  author = {Fernando G. S. L. Brandão and Aram W. Harrow},
  title = {Product-state Approximations to Quantum Ground States},
  journal = {Proc. of the 45th ACM Symposium on theory of STOC},
