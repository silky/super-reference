@ARTICLE{Aubrun2013,
  author = {Guillaume Aubrun and Cécilia Lancien},
  title = {On the distinguishability norms in quantum information},
  journal = {arXiv:1309.6003v1},
  year = {2013},
  month = {9},
  note = {arXiv:1309.6003v1},
  abstract = {We study the "distinguishability norms" associated to either a single
	POVM or a family of POVMs. These norms quantify how quantum measurements
	perform in the task of discriminating two quantum states. We address
	the following questions regarding their behaviour in the high-dimensional
	regime: (i) How many POVMs are needed to approximate the family of
	all POVMs on C^d? We show that the answer is exp(O(d^2)). (ii) How
	many outcomes are required for a POVM to emulate the uniform POVM
	on C^d? Improving on previous results, we show that O(d^2) outcomes
	are necessary and sufficient. Moreover, using the concept of zonoid
	and approximation results from Banach space theory, we show that
	this property extends (in a weaker form) to an arbitrary POVM. (iii)
	On a bipartite space, what are the relative strengths of standard
	classes of locally restricted measurements? We show that the class
	of PPT measurements typically performs almost as well as the class
	of all measurements whereas restricting to separable measurements
	implies a substantial loss.},
  file = {Aubrun2013, 1309.6003.pdf:Aubrun2013, 1309.6003.pdf:PDF},
  timestamp = {2013.09.27},
