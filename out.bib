}
@article{
  author = {Ali Nabi Duman},
  url = {http://arxiv.org/abs/1405.4428v1},
  timestamp = {2014.12.29},
  abstract = {Categorical quantum mechanics, which examines quantum theory via dagger-compact closed categories, gives satisfying high-level explanations to the quantum information procedures such as Bell-type entanglement or complementary observables (\cite{AC}, \cite{Co}, \cite{Co2}). Inspired by the fact that Quantum Game Theory can be seen as branch of quantum information, we express Quantum Game Theory procedures using the topological semantics provided by Categorical Quantum Mechanics. We also investigate Bayesian Games with correlation from this novel point of view while considering the connection between Bayesian game theory and Bell non-locality investigated recently by Brunner and Linden \cite{BL}.},
