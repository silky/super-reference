@ARTICLE{Kato2013,
  author = {Kohtaro Kato and Fabian Furrer and Mio Murao},
  title = {Operational Meaning of Entanglement Entropy in Anyonic Systems},
  journal = {arXiv:1310.4140v1},
  year = {2013},
  month = {10},
  note = {arXiv:1310.4140v1},
  abstract = {We present an operationally motivated analysis of bipartite entanglement
	in anyonic systems by introducing an entanglement measure and link
	it to optimal distillation and dilution rates for pure states. The
	measure which we call the anyonic entanglement entropy is defined
	as the asymptotic limit of infinite identical copies of the straightforward
	generalization of the entanglement entropy to anyonic systems. We
	show that it evaluates to the straightforward generalization of the
	entanglement entropy of a single copy plus a correction term depending
	only on the quantum dimensions of the anyons. We further prove that
	the anyonic entanglement entropy satisfies all required properties
	of an entanglement measure. Moreover, it characterizes the asymptotically
	optimal distillation and dilution rates similar to the usual entanglement
	entropy for qubit systems.},
  file = {Kato2013, 1310.4140v1.pdf:Kato2013, 1310.4140v1.pdf:PDF},
