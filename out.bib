@ARTICLE{Bina2013,
  author = {Matteo Bina and Antonio Mandarino and Stefano Olivares and Matteo
	G. A. Paris},
  title = {About the use of fidelity to assess quantum resources},
  journal = {arXiv:1309.5325v1},
  year = {2013},
  month = {9},
  note = {arXiv:1309.5325v1},
  abstract = {Fidelity is a figure of merit widely employed in quantum technology
	in order to quantify similarity between quantum states and, in turn,
	to assess quantum resources or reconstruction techniques. Fidelity
	higher than, say, 0.9 or 0.99 are, in fact, usually considered as
	a piece of evidence to say that two states i) are very close in the
	Hilbert space; and ii) share nearly identical physical properties.
	Here, on the basis of several examples for qubits and continuous
	variable systems, we show that while the first statement is certainly
	true, "very high" level of fidelity may be achieved by pair of states
	with considerably different physical properties, including separable
	and entangled states or classical and nonclassical ones. We conclude
	that fidelity as a tool to assess quantum resources should be employed
	with caution, possibly combined with additional constraints restricting
	the pool of achievable states, or only as a mere summary of a full
	tomographic reconstruction.},
  file = {Bina2013, 1309.5325.pdf:Bina2013, 1309.5325.pdf:PDF},
  timestamp = {2013.09.27},
  url = {http://arxiv.org/abs/1309.5325v1}
}
