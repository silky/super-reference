@ARTICLE{Berta2014a,
  author = {Mario Berta and Kaushik P. Seshadreesan and Mark M. Wilde},
  title = {Renyi generalizations of the conditional quantum mutual information},
  journal = {arXiv:1403.6102v2},
  year = {2014},
  month = {3},
  note = {arXiv:1403.6102v2},
  abstract = {The conditional quantum mutual information $I(A;B|C)$ of a tripartite
	state $\rho_{ABC}$ is an information quantity which lies at the center
	of many problems in quantum information theory. Three of its main
	properties are that it is non-negative for any tripartite state,
	that it decreases under local operations applied to systems $A$ and
	$B$, and that it obeys the duality relation $I(A;B|C)=I(A;B|D)$ for
