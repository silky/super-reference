@ARTICLE{Jozsa2014,
  author = {Richard Jozsa and Graeme Mitchison},
  title = {Symmetric polynomials in information theory: entropy and subentropy},
  journal = {arXiv:1404.7694v1},
  year = {2014},
  month = {4},
  note = {arXiv:1404.7694v1},
  abstract = {Entropy and other fundamental quantities of information theory are
	customarily expressed and manipulated as functions of probabilities.
	Here we study the entropy H and subentropy Q as functions of the
	elementary symmetric polynomials in the probabilities, and reveal
	a series of remarkable properties. Derivatives of all orders are
	shown to satisfy a complete monotonicity property. H and Q themselves
	become multivariate Bernstein functions and we derive the density
	functions of their Levy-Khintchine representations. We also show
	that H and Q are Pick functions in each symmetric polynomial variable
	separately. Furthermore we see that H and the intrinsically quantum
	informational quantity Q become surprisingly closely related in functional
	form, suggesting a special significance for the symmetric polynomials
	in quantum information theory. Using the symmetric polynomials we
	also derive a series of further properties of H and Q.},
  file = {Jozsa2014, 1404.7694.pdf:Jozsa2014, 1404.7694.pdf:PDF},
  timestamp = {2014.05.14},
  url = {http://arxiv.org/abs/1404.7694v1}
}
@ARTICLE{Jozsa2013a,
  author = {Richard Jozsa and Graeme Mitchison},
  title = {Entropy, subentropy and the elementary symmetric functions},
  journal = {arXiv:1310.6629v1},
  year = {2013},
  month = {10},
  note = {arXiv:1310.6629v1},
  abstract = {We use complex contour integral techniques to study the entropy H
	and subentropy Q as functions of the elementary symmetric polynomials,
	revealing a series of striking properties. In particular for these
	variables, derivatives of -Q are equal to derivatives of H of one
	higher order and the first derivatives of H and Q are seen to be
	completely monotone functions. It then follows that exp (-H) and
	exp(-Q) are Laplace transforms of infinitely divisible probability
	distributions.},
  file = {Jozsa2013a, 1310.6629.pdf:Jozsa2013a, 1310.6629.pdf:PDF},
  timestamp = {2013.10.27},
  url = {http://arxiv.org/abs/1310.6629v1}
}
@ARTICLE{Jozsa2013b,
  author = {Richard Jozsa and Akimasa Miyake},
  title = {Jordan-Wigner formalism for classical simulation beyond binary matchgates},
  journal = {arXiv:1311.3046v1},
  year = {2013},
  month = {11},
  note = {arXiv:1311.3046v1},
  abstract = {The unitary matchgate circuits introduced by Valiant provide an interesting
	class of quantum circuits that are classically efficiently simulatable.
	They were shown by Terhal & DiVincenzo and Knill to be related to
	the physics of non-interacting fermions. The Jordan-Wigner (JW) formalism
	provides an efficient classical simulation of the latter, which turns
	out to be equivalent to the restricted case of circuits of binary
	(2-qubit) matchgates. Valiant's formalism allows further unitary
	gates: in particular we may include arbitrary 1-qubit gates on the
	first qubit line at any stage within a binary matchgate circuit.
	In this note we show how the JW formalism may be extended to provide
	an efficient classical simulation of such extended circuits, and
	we show how the simulability also follows from some elementary Lie
	algebra theory. The essential ingredients have been indicated previously
	by Knill in a condensed and abstract form, and our purpose is to
	make these results explicit and transparent.},
  file = {Jozsa2013b, 1311.3046.pdf:Jozsa2013b, 1311.3046.pdf:PDF},
  timestamp = {2013.11.22},
  url = {http://arxiv.org/abs/1311.3046v1}
}
@ARTICLE{Jozsa2013,
  author = {Richard Jozsa and Maarten Van den Nest},
  title = {Classical simulation complexity of extended Clifford circuits},
  journal = {arXiv:1305.6190v1},
  year = {2013},
  month = {5},
  note = {arXiv:1305.6190v1},
  abstract = {Clifford gates are a winsome class of quantum operations combining
	mathematical elegance with physical significance. The Gottesman-Knill
	theorem asserts that Clifford computations can be classically efficiently
	simulated but this is true only in a suitably restricted setting.
	Here we consider Clifford computations with a variety of additional
	ingredients: (a) strong vs. weak simulation, (b) inputs being computational
	basis states vs. general product states, (c) adaptive vs. non-adaptive
	choices of gates for circuits involving intermediate measurements,
	(d) single line outputs vs. multi-line outputs. We consider the classical
	simulation complexity of all combinations of these ingredients and
	show that many are not classically efficiently simulatable (subject
	to common complexity assumptions such as P not equal to NP). Our
	results reveal a surprising proximity of classical to quantum computing
	power viz. a class of classically simulatable quantum circuits which
	yields universal quantum computation if extended by a purely classical
	additional ingredient that does not extend the class of quantum processes
	occurring.},
  file = {Jozsa2013, 1305.6190.pdf:Jozsa2013, 1305.6190.pdf:PDF},
  timestamp = {2013.05.30},
  url = {http://arxiv.org/abs/1305.6190}
}
@ARTICLE{Juenemann2013,
  author = {J. Juenemann and A. Cadarso and D. Perez-Garcia and A. Bermudez and
	J. J. Garcia-Ripoll},
  title = {Lieb-Robinson bounds for spin-boson lattice models and trapped ions},
  journal = {arXiv:1307.1992v1},
