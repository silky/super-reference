  journal = {arXiv:1309.2514v1},
  year = {2013},
  month = {9},
  note = {arXiv:1309.2514v1},
  abstract = {We report on the observation of quantum interference of a collective
	single spin excitation with a spin ensemble of N_atom =10^5 atoms.
