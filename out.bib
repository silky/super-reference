}
@ARTICLE{Cao2013,
  author = {Yudong Cao and Ryan Babbush and Jacob Biamonte and Sabre Kais},
  title = {Experimentally Realizable Hamiltonian Gadgets},
  journal = {arXiv:1311.2555v1},
  year = {2013},
  month = {11},
  note = {arXiv:1311.2555v1},
  abstract = {Adiabatic quantum computation works by encoding problem instances
	into Hamiltonians. Experimental systems are typically limited to
	two-body interactions. Therefore, one needs a method to approximate
	quantum many-body Hamiltonians up to arbitrary spectral error using
	at most two-body interactions. Hamiltonian gadgets, introduced over
	a decade ago, offer the only current means to address this requirement
	for general many-body Hamiltonians. Although the applications of
	Hamiltonian gadgets have steadily grown since their introduction,
	little progress has been made in overcoming the limitations of the
	gadgets themselves. In this experimentally motivated theoretical
	study, we propose several gadgets which have control parameters within
	reach of existing experimental hardware. We employ analytical techniques
	which result in a reduction of the resource scaling as a function
	of spectral error for the commonly used subdivision and 3- to 2-body
	gadgets. Next, we numerically optimize these new gadgets and fully
	characterize how the spectral gap scales with increasing locality
	in all known gadget constructions. Our analysis points to regimes
	and trade-offs where various constructions are shown to afford advantages.
	Finally, we introduce a new creation gadget which allows for the
	realization of any real-valued interaction using only XX and XZ couplers.
	This work represents a required step forward for the eventual realization
	of a universal adiabatic quantum information processor and paves
	the way for a first experimental implementation of these key building
	blocks by requiring less control precision without introducing extra
	ancillary qubits.},
  file = {Cao2013, 1311.2555.pdf:Cao2013, 1311.2555.pdf:PDF},
  timestamp = {2013.11.22},
  url = {http://arxiv.org/abs/1311.2555v1}
}
@ARTICLE{Carlen2013,
  author = {Eric A. Carlen and Joel L. Lebowitz and Elliott H. Lieb},
  title = {On an Extension Problem for Density Matrices},
  journal = {arXiv:1301.4605v1},
  year = {2013},
  month = {1},
  note = {arXiv:1301.4605v1},
  abstract = {We investigate the problem of the existence of a density matrix rho
	on the product of three Hilbert spaces with given marginals on the
