@ARTICLE{Kalev2013,
  author = {Amir Kalev},
  title = {A geometrical relation between symmetric operators and mutually unbiased
	operators},
  journal = {arXiv:1305.6044v1},
  year = {2013},
  month = {5},
  note = {arXiv:1305.6044v1},
  abstract = {In this work we study the relation between the set of symmetric operators
