  url = {http://arxiv.org/abs/1308.1306v1}
}
@ARTICLE{Chen2013d,
  author = {Lin Chen and Dragomir Z. Djokovic and Markus Grassl and Bei Zeng},
  title = {Four-qubit pure states as fermionic states},
  journal = {arXiv:1309.0791v1},
  year = {2013},
  month = {9},
  note = {arXiv:1309.0791v1},
  abstract = {The embedding of the $n$-qubit space into the $n$-fermion space with
	$2n$ modes is a widely used method in studying various aspects of
	these systems. This simple mapping raises a crucial question: does
	the embedding preserve the entanglement structure? It is known that
	the answer is affirmative for $n=2$ and $n=3$. That is, under either
	local unitary (LU) operations or with respect to stochastic local
	operations and classical communication (SLOCC), there is a one-to-one
	correspondence between the 2- (or 3)-qubit orbits and the 2- (or
	3)-fermion orbits with 4 (or 6) modes. However these results do not
	generalize as the mapping from the $n$-qubit orbits to the $n$-fermion
	orbits with $2n$ modes is no longer surjective for $n>3$. Here we
	consider the case of $n=4$. We show that surprisingly, the orbit
	mapping from qubits to fermions remains injective under SLOCC, and
	a similar result holds under LU for generic orbits. As a byproduct,
	we obtain a complete answer to the problem of SLOCC equivalence of
	pure 4-qubit states.},
  file = {Chen2013d, 1309.0791.pdf:Chen2013d, 1309.0791.pdf:PDF},
  timestamp = {2013.09.06},
  url = {http://arxiv.org/abs/1309.0791v1}
}
@ARTICLE{Chen2013e,
  author = {Lin Chen and Oleg Gittsovich and Kavan Modi and Marco Piani},
  title = {Role of correlations in the two-body marginals problem},
  journal = {arXiv:1310.1110v1},
  year = {2013},
  month = {10},
  note = {arXiv:1310.1110v1},
  abstract = {We address the issue of deducing the global properties of correlations
	of tripartite quantum states based on the knowledge of their bipartite
	reductions, with special focus on the case of three qubits. We aim
	at relating the properties of the correlations in the bipartite reductions
	to global correlation properties. We prove that strictly classical
	bipartite correlations may still require global entanglement. Furthermore,
	we prove that separable---albeit not strictly classical---reductions
	may require global genuine multipartite entanglement, rather than
	simple entanglement. On the other hand, for three qubits, the strict
	classicality of the bipartite reductions rules out the need for genuine
	multipartite entanglement. Our work sheds new light on the relation
	between local and global properties of quantum states, and on the
	interplay between classical and quantum properties of correlations.},
  file = {Chen2013e, 1310.1110.pdf:Chen2013e, 1310.1110.pdf:PDF},
  timestamp = {2013.10.11},
  url = {http://arxiv.org/abs/1310.1110v1}
}
@ARTICLE{Chen2014a,
  author = {Lin Chen and Li Yu},
  title = {Nonlocal and controlled unitary operators of Schmidt rank three},
  journal = {arXiv:1403.5720v1},
  year = {2014},
  month = {3},
  note = {arXiv:1403.5720v1},
  abstract = {Implementing nonlocal unitary operators is an important and hard question
	in quantum computing and cryptography. We show that any bipartite
	nonlocal unitary operator of Schmidt rank 3 on the $(d_A \times d_B)$-dimensional
	system is locally equivalent to a controlled unitary when $d_A$ is
	at most $3$. This operator can be locally implemented assisted by
	a maximally entangled state of Schmidt rank $r=\min\{d_A^2,d_B\}$.
	We further show that stochastic-equivalent nonlocal unitary operators
	are indeed locally equivalent, and propose a sufficient condition
	on which nonlocal and controlled unitary operators are locally equivalent.
	We also provide the solution to a special case of a conjecture on
	the ranks of multipartite quantum states.},
  file = {Chen2014, 1403.5720.pdf:Chen2014, 1403.5720.pdf:PDF},
  timestamp = {2014.04.21},
  url = {http://arxiv.org/abs/1403.5720v1}
}
@ARTICLE{Chen2013i,
  author = {Moran Chen and Nicolas C. Menicucci and Olivier Pfister},
  title = {Experimental realization of multipartite entanglement of 60 modes
	of the quantum optical frequency comb},
  journal = {arXiv:1311.2957v2},
  year = {2013},
  month = {11},
  note = {arXiv:1311.2957v2},
  abstract = {We report the experimental realization and characterization of one
	60-mode copy, and of two 30-mode copies, of a dual-rail quantum-wire
	cluster state in the quantum optical frequency comb of a bimodally
	pumped optical parametric oscillator. This is the largest entangled
