  year = {2014},
  month = {2},
  note = {arXiv:1402.7254v2},
  abstract = {In the distributed {\em Deutsch-Jozsa promise problem}, two parties
	are to determine whether their respective strings $x,y\in\{0,1\}^n$
	are at the Hamming distance $H(x,y)=0$ or $H(x,y)=\frac{n}{2}$. Buhrman
	et al. (STOC' 98) proved that the exact {\em quantum communication
	complexity} of this problem is ${\bf O}(\log {n})$ while the {\em
	deterministic communication complexity} is ${\bf \Omega}(n)$. This
	was the first impressively (exponential) gap between quantum and
	classical communication complexity. In this paper, we generalize
	the above distributed Deutsch-Jozsa promise problem to determine
	for a fixed $k\geq \frac{n}{2}$ whether $H(x,y)=0$ or $H(x,y)= k$.
	We will also discuss the promise versions of the well-known {\em
	disjointness} problem. Applications to {\em finite automata} of the
	results obtained are also shown in this paper.},
  file = {Gruska2014, 1402.7254.pdf:Gruska2014, 1402.7254.pdf:PDF},
  timestamp = {2014.04.21},
  url = {http://arxiv.org/abs/1402.7254v2}
}
