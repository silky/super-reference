@ARTICLE{Roberson2012,
  author = {David E. Roberson and Laura Mancinska},
  title = {Graph Homomorphisms for Quantum Players},
  journal = {arXiv:1212.1724v1},
  year = {2012},
  month = {12},
  note = {arXiv:1212.1724v1},
  abstract = {A homomorphism from a graph X to a graph Y is an adjacency preserving
	mapping f:V(X)->V(Y). We consider a nonlocal game in which Alice
	and Bob are trying to convince a verifier with certainty that a graph
	X admits a homomorphism to Y. This is a generalization of the well-studied
