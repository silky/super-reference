@ARTICLE{Jencova2014,
  author = {Anna Jencova},
  title = {Randomization theorems for quantum channels},
  journal = {arXiv:1404.3900v1},
  year = {2014},
  month = {4},
  note = {arXiv:1404.3900v1},
  abstract = {The classical randomization criterion is an important result of statistical
	decision theory. Recently, a quantum analogue has been proposed,
	giving equivalent conditions for two sets of quantum states, ensuring
	existence of a quantum channel mapping one set close to the other,
	in $L_1$-distance. In the present paper, we extend these concepts
