@article{
  author = {James D. Whitfield and Zoltán Zimborás},
  url = {http://arxiv.org/abs/1408.3459v1},
  timestamp = {2014.12.29},
  abstract = {The self-consistent field method utilized for solving the Hartree-Fock (HF) problem and the closely related Kohn-Sham problem, is typically thought of as one of the cheapest methods available to quantum chemists. This intuition has been developed from the numerous applications of the self-consistent field method to a large variety of molecular systems. However, in terms of its worst-case computational complexity, the HF problem is NP-complete. In this work, we investigate how far one can push the boundaries of the NP-completeness by investigating restricted instances of HF. We have constructed two new NP-complete variants of the problem. The first is a set of Hamiltonians whose translationally invariant Hartree-Fock solutions are trivial, but whose broken symmetry solutions are NP-complete. Second, we demonstrate how to embed instances of spin glasses into translationally invariant Hartree-Fock instances and provide a numerical example. These findings are the first steps towards understanding in which cases the self-consistent field method is computationally feasible and when it is not.},
  title = {On the NP-completeness of the Hartree-Fock method for translationally invariant systems},
  month = {8},
  note = {arXiv:1408.3459v1},
  file = {Whitfield2014, 1408.3459.pdf:Whitfield2014, 1408.3459.pdf:PDF},
  year = {2014},
  journal = {arXiv:1408.3459v1}
}
@article{
  author = {Keiji Matsumoto},
  url = {http://arxiv.org/abs/1408.3462v1},
  timestamp = {2014.12.29},
  abstract = {We study tree kinds of quantum fidelity. Usual Uhlmann's fidelity, minus of f-divergence when $f(x)=-\sqrt{x}$, and the one introduced by the author via reverse test. All of them are quantum extensions of classical fidelity, where the first one is the largest and the third one is the smallest. We characterize them in terms of convex optimization, and introduce their 'dual' quantity, or the polar of the minus of the fidelity. They turned out to be monotone increasing by unital completely positive maps, concave, and linked to its classical version via optimization about classical-to-quantum maps and quantum-to-classical maps.},
  title = {Quantum Fidelities, Their Duals, And Convex Analysis},
  month = {8},
