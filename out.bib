@ARTICLE{Gualdi2014,
  author = {Giulia Gualdi and David Licht and Daniel M. Reich and Christiane
	P. Koch},
  title = {Efficient Monte Carlo characterization of quantum operations for
	qudits},
  journal = {arXiv:1404.1608v1},
  year = {2014},
  month = {4},
  note = {arXiv:1404.1608v1},
  abstract = {For qubits, Monte Carlo estimation of the average fidelity of Clifford
	unitaries is efficient -- it requires a number of experiments that
	is independent of the number $n$ of qubits and classical computational
	resources that scale only polynomially in $n$. Here, we identify
